{
    "name": "Dewey: Bookmark searcher",
    "version": "1.0.0",
    "description": "Search your bookmarks with ease!",
    "manifest_version": 3,
    "author": "jsattari",
    "icons": {
        "16": "icon_sm.png",
        "48": "icon_lrg.png"
    },
    "permissions": [
        "bookmarks"
    ],
    "action": {
        "default_icon": "icon_lrg.png",
        "default_popup": "index.html",
        "default_title": "Dewey"
    }
}
